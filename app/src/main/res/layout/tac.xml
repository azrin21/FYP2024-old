<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <TextView
            style="@style/DialogFragmentBody"
            android:layout_marginTop="@dimen/margin_48dp"
            android:text="TAC Number"
            android:textColor="@color/Black" />

        <EditText
            android:id="@+id/editTextTac"
            style="@style/DialogFragmentEditText"
            android:inputType="number" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="end"
            android:layout_marginTop="@dimen/margin_8dp"
            android:layout_marginBottom="@dimen/margin_8dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/btnCancelTac"
                style="@style/DialogFragmentButtonCancel" />

            <Button
                android:id="@+id/buttonSubmitTac"
                style="@style/DialogFragmentButtonOk" />
        </LinearLayout>
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>